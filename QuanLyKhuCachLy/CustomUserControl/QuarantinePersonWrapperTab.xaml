<UserControl x:Class="QuanLyKhuCachLy.CustomUserControl.QuarantinePersonWrapperTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuanLyKhuCachLy.CustomUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContext="{StaticResource PersonVM}"
             >
    <Grid>
        <local:ListQuarantinedPerson Visibility="{Binding TabList}"></local:ListQuarantinedPerson>
        <local:QuarantinedPersonInformation Visibility="{Binding TabInformation}"/>
        
    </Grid>
</UserControl>
